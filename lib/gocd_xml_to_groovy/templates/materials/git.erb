git<%= "('#{material.attributes.name}')" if material.attributes.name %> {
          <%- filtered_attributes(material.attributes, :url, :branch, :shallowClone, :destination, :autoUpdate, _ignore: [:name, :filter, :invert_filter, :submodule_folder]) do |k, v| -%>
          <%= k %> = <%= v %>
          <%- end -%>
          <%= render 'materials/filter.erb', filter: material.attributes.filter, invert_filter: material.attributes.invert_filter if material.attributes.filter %>
        }